<!-- Left side column. contains the logo and sidebar -->
<!-- Right side column. Contains the navbar and content of the page -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <!-- left column -->
            <div class="col-md-6 col-sm-6">
                <!-- general form elements -->
                <div class="box box-primary">
                    <div class="box-header">
                        <h3 class="box-title">Create Markout</h3>
                    </div><!-- /.box-header -->
                    <!-- form start -->
                    <form class="@Url.Action("Create", "mark_out")" method="POST">
                        <div class="box-body">
                            <div class="form-group">
                                <label for="exampleInputEmail1">Reference_number</label>
                                <input type="text" class="form-control" id="Reference_number" name="Reference_number" placeholder=" enter reference_number" required>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Title</label>
                                <input type="text" class="form-control" name="File_title" placeholder="Enter Title" required>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEmail1">Volume_number</label>
                                <input type="text" class="form-control" name="Volume_number" placeholder="Enter Volume_number" required>
                            </div>
                            
                            
                            
                          
                              <div class="form-group">
                                <label for="exampleInputEmail1">File_status</label>
                                
                                    <select name="File_status">
                                        <option value="current">current</option>
                                        <option value="sem_current">sem_current</option>
                                        <option value="non_current">non_current</option>

                                    </select>
                                </div>
                               
                               
                            </div>
                           
                            <div class="form-group">
                                <label for="exampleInputEmail1">Requesting_officer</label>
                                <input type="text" class="form-control" name="Requesting_officer" placeholder="Enter Requesting_officer" required>
                            </div>
                            

                        </div><!-- /.box-body -->
                        <!-- /.box-body -->
                        <div class="box-footer">
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                @if (null != TempData["alert"])
                {
                    <script type="text/javascript">
                        alert("@TempData["alert"]");
                    </script>
                }
                    </form>
                </div><!-- /.box -->
              


            </div><!--/.col (left) -->
            <!-- right column -->
            <!--/.col (right) -->
        </div>   <!-- /.row -->
    </section><!-- /.content -->
</div><!-- /.content-wrapper -->
