@model File_tracking_system.mark_out

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12 col-sm-12">
                <!-- general form elements -->
                <div class="box box-primary">
                    <div class="box-header">
                        <h3 class="box-title">Delete Markout</h3>
                    </div><!-- /.box-header -->
                    <!-- form start -->
                    <form class="@Url.Action("Delete", "mark_out")" method="POST">
                        <div class="box-body">
                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("Reference_number")
                                </div>
                                <div class="editor-field">
                                    @Html.TextBoxFor(model => model.Reference_number, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Reference_number)
                                </div>
                            </div>
                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("Title")
                                </div>
                                <div class="editor-field">
                                    @Html.TextBoxFor(model => model.File_title, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.File_title)
                                </div>
                            </div>

                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("Date_in")
                                </div>
                                <div class="editor-field">
                                    @Html.TextBoxFor(model => model.Date_in, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Date_in)
                                </div>
                            </div>

                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("Date_out")
                                </div>
                                <div class="editor-field">
                                    @Html.TextBoxFor(model => model.Date_out, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Date_out)
                                </div>
                            </div>
                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label(" File status")
                                </div>
                                <div class="col-md-6">
                                    <select name="File_status">
                                        <option value="current">current</option>
                                        <option value="sem_current">sem_current</option>
                                        <option value="outdated">outdated</option>

                                    </select>
                                </div>
                            </div>

                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("Requesting_officer")
                                </div>
                                <div class="editor-field">
                                    @Html.TextBoxFor(model => model.Requesting_officer, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Requesting_officer)
                                </div>
                            </div>
                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("volume number")
                                </div>
                                <div class="editor-field">
                                    @Html.TextBoxFor(model => model.Volume_number, new { @class = "form-control" })
                                    @Html.ValidationMessageFor(model => model.Volume_number)
                                </div>
                            </div>
                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("Markout File status")
                                </div>
                                <div class="col-md-6">
                                    <select name="Markout_File_status">
                                        <option value="In">In</option>
                                        <option value="Out">Out</option>

                                    </select>
                                </div>
                            </div>
                            <div class="col-md-6">

                                <div class="editor-label">
                                    @Html.Label("duration_status")
                                </div>
                                <div class="col-md-6">
                                    <select name="Markout_File_status">
                                        <option value="--">--</option>


                                    </select>
                                </div>
                            </div>
                            <div>
                                @Html.HiddenFor(model => model.Request_number)
                            </div>

                        </div><!-- /.box-body -->
                        <!-- /.box-body -->
                        <div class="box-footer align-content-center">
                            <button type="submit" class="btn btn-primary">Delete</button>
                        </div>
                    </form>
                </div><!-- /.box -->


            </div><!--/.col (left) -->
            <!-- right column -->
            <!--/.col (right) -->
        </div>   <!-- /.row -->
    </section><!-- /.content -->
</div><!-- /.content-wrapper -->
<!-- ./wrapper -->
<!-- jQuery 2.1.3 -->
<script src="~/Content/adm/plugins/jQuery/jQuery-2.1.3.min.js"></script>
<!-- Bootstrap 3.3.2 JS -->
<script src="~/Content/adm/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<!-- FastClick -->
<script src='~/Content/adm/plugins/fastclick/fastclick.min.js'></script>
<!-- AdminLTE App -->
<script src="~/Content/adm/dist/js/app.min.js" type="text/javascript"></script>
<!-- AdminLTE for demo purposes -->
<script src="~/Content/adm/dist/js/demo.js" type="text/javascript"></script>

