@model IEnumerable<File_tracking_system.mark_out>
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            REPORT TABLE

        </h1>

    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <!-- /.box -->
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Non-Current Files Markout Report</h3>
                    </div><!-- /.box-header -->
                    <div class="box-body">
                        <table id="example1" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>FileName</th>
                                    <th>ReferenceNumber</th>

                                    <th>DateIN</th>
                                    <th>DateOut</th>
                                    <th>Dispatch_officer</th>

                                    <th>Requesting_officer</th>


                                </tr>
                            </thead>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.File_title)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Reference_number)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.Date_in)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Date_out)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Dispatch_officer)
                                    </td>

                                    <td>
                                        @Html.DisplayFor(modelItem => item.Requesting_officer)
                                    </td>



                                </tr>
                            }
                            <tfoot>
                                <tr>
                                    <th>FileName</th>
                                    <th>ReferenceNumber</th>

                                    <th>DateIN</th>
                                    <th>DateOut</th>

                                    <th>Dispatch_officer</th>
                                    <th>Requesting_officer</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div><!-- /.box-body -->
                </div><!-- /.box -->
            </div><!-- /.col -->
        </div><!-- /.row -->
    </section><!-- /.content -->
</div><!-- /.content-wrapper -->
<!-- ./wrapper -->
<!-- jQuery 2.1.3 -->
<script src="~/Content/adm/plugins/jQuery/jQuery-2.1.3.min.js"></script>
<!-- Bootstrap 3.3.2 JS -->
<script src="~/Content/adm/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<!-- DATA TABES SCRIPT -->
<script src="~/Content/adm/plugins/datatables/jquery.dataTables.js" type="text/javascript"></script>
<script src="~/Content/adm/plugins/datatables/dataTables.bootstrap.js" type="text/javascript"></script>
<!-- SlimScroll -->
<script src="~/Content/adm/plugins/slimScroll/jquery.slimscroll.min.js" type="text/javascript"></script>
<!-- FastClick -->
<script src='~/Content/adm/plugins/fastclick/fastclick.min.js'></script>
<!-- AdminLTE App -->
<script src="~/Content/adm/dist/js/app.min.js" type="text/javascript"></script>
<!-- AdminLTE for demo purposes -->
<script src="~/Content/adm/dist/js/demo.js" type="text/javascript"></script>
<!-- page script -->
<script type="text/javascript">
    $(function () {
        $("#example1").dataTable();
        $('#example2').dataTable({
            "bPaginate": true,
            "bLengthChange": false,
            "bFilter": false,
            "bSort": true,
            "bInfo": true,
            "bAutoWidth": false
        });
    });
</script>


